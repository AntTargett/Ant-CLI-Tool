{
  "name": "imanant",
  "version": "1.8.4",
  "description": "Node CLI tool that prints Ant aski art :D",
  "main": "main.js",
  "bin": {
    "imanant": "main.js"
  },
  "scripts": {
    "test": "echo \"Test has been run\" && exit 1",
    "pretty": "prettier --write --tab-width 4 \"command-actions/**/*.js\""
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "npm run pretty",
      "npm run test",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AntTargett/CLI-Tools.git"
  },
  "author": "Antony Targett",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AntTargett/CLI-Tools/issues"
  },
  "homepage": "https://github.com/AntTargett/CLI-Tools#readme",
  "dependencies": {
    "commander": "^2.16.0"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.14.0"
  }
}
